         <td>
           <% for(var i in googles){ %>
                <% if (i< 20){ %>
                           <a style="font-size:12px;" href="http://google.com/search?q=<%= googles[i].q %>"><%= googles[i].show %></a><br/>
                 <% }else{ %>
                          <div class="hidegoogle" style="display:none;"><a style="font-size:12px;" href="http://google.com/search?q=<%= googles[i].q %>"><%= googles[i].show %></a></div>
                 <% } %>
            <% } %>
            <a href="#" id="moregoogles">show more googles..</a>
         </td>         
         -->

             <td>
       <% for(var i in maplist){ %>
              <img src="<%= maplist[i].image %>" title="<%= maplist[i].q %>" style="width:125px; height:125px;"/>
              <br/>
              <p></p>

             <% } %>
          </td> 
         </tr>
         <tr style="height:150px;">
         </tr>
         </table>
 
 
 
 <script>

        $(".domain").click(function(){
            $(this).siblings(".sites:first").toggle();
              return false;
        })    
        
          $("#moregoogles").click(function(){
            $(".hidegoogle").each(function(){$(this).toggle();})
              return false;
        })    

 </script>
