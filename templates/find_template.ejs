
<table style="position:absolute; vertical-align:top; top:200px; left:25%; min-width:200px; min-height:200px; ">
  <tr style="vertical-align:top;">
  <Td style="vertical-align:top;">
  
  <input type="text" style=" top:0px; background:black; opacity:0.3; width:80%; height:30px; color:white; border-radius:5px;" id="recentsearch" />
         <table id="domainlist" style=" overflow-y:scroll; background:white; opacity:0.6; border-radius: 5px"> 
          <% for(var i in tabs){ %>
            <tr>
             <td>  
                <!--<img style="width:15px; height:15px;" src="<%= tabs[i].favicon %>" / >-->
                <a href="#" style="font-size:<%= tabs[i].font %>;" class="domain"> <%= tabs[i].domain %></a>  </br> 
                  <div class="sites" id="<%= tabs[i].domain %>" style="display:none; position:relative; left:100px;">      
                    <% for(var o in tabs[i].sites){ %>
                      <a href="<%= tabs[i].sites[o].url %>" style="" > <%= tabs[i].sites[o].title %></a></br>
                    <% } %>
                  </div>
             </td>
            </tr>
          <% } %>    
         </table>
         
         
         </td>
